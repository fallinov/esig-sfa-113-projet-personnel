---
export interface Props {
  icon: string;
  title: string;
  items: string[];
}

const { icon, title, items } = Astro.props;
---

<div class="livrable-card">
    <div class="livrable-icon">{icon}</div>
    <h3>{title}</h3>
    <ul>
        {items.map(item => (
            <li>{item}</li>
        ))}
    </ul>
</div>
